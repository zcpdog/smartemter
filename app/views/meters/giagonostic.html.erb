<div style="width: 700px;margin: 0 auto">
<%=link_to "Home", root_path%> | <%=link_to "Back", :back%><br>
<%=render "commons/date_selector"%>
<h1>Smatmeter Giagonostic</h1>
<hr>
<table class="pure-table pure-table-bordered data-table" data-method="giagonostic" data-id="<%=@meter.id%>">
    <thead>
        <tr>
            <th>#</th>
			<th>Record Time</th>
            <th>Status</th>
        </tr>
    </thead>
	<tbody>
		<%@meter_records.each do |record|%>
	        <tr>
	            <td><%=record.id%></td>
	            <td><%=record.created_at%></td>
	            <td>Normal</td>
	        </tr>
		<%end%>
    </tbody>
</table>
</div>